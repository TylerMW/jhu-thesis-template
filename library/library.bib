keywords = {Animals,Biophysics,Biophysics: methods,COS Cells,Cercopithecus aethiops,Cytological Techniques,Cytoplasm,Cytoplasm: metabolism,Elasticity,Microscopy, Video,Microscopy, Video: methods,Models, Biological,Oligonucleotide Probes,Oligonucleotide Probes: metabolism,Oocytes,Oocytes: metabolism,Particle Size,Rheology,Viscosity,Xenopus laevis,Xenopus laevis: metabolism},
month = may,
