keywords = {Actins,Actins: chemistry,Actins: ultrastructure,Artifacts,Biopolymers,Biopolymers: chemistry,Elasticity,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Microscopy,Microscopy: methods,Molecular Motor Proteins,Molecular Motor Proteins: chemistry,Molecular Motor Proteins: ultrastructure,Motion,Protein Conformation,Reproducibility of Results,Sensitivity and Specificity,Stress, Mechanical},
month = jul,
number = {1},
pages = {346--59},
pmid = {17416612},
title = {{Bending dynamics of fluctuating biopolymers probed by automated high-resolution filament tracking.}},
