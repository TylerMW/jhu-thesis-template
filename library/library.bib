issn = {0927-7765},
journal = {Colloids and surfaces. B, Biointerfaces},
keywords = {Acinetobacter,Acinetobacter: physiology,Alkanes,Alkanes: chemistry,Alkanes: metabolism,Elastic Modulus,Emulsions,Hydrophobic and Hydrophilic Interactions,Rhodococcus,Rhodococcus: physiology,Surface Properties,Water,Water: chemistry,Water: metabolism},
month = nov,
