journal = {Journal of controlled release : official journal of the Controlled Release Society},
keywords = {Curcumin,Curcumin: chemistry,Drug Compounding,Drug Delivery Systems,Female,Humans,Mucus,Mucus: chemistry,Nanoparticles,Nanoparticles: chemistry,Permeability,Polyethylene Glycols,Polyethylene Glycols: chemistry,Polyglactin 910,Polyglactin 910: chemistry,Polyvinyl Alcohol,Polyvinyl Alcohol: chemistry},
month = sep,
number = {2},
pages = {279--86},
pmid = {23751567},
title = {{Scalable method to produce biodegradable nanoparticles that rapidly penetrate human mucus.}},
