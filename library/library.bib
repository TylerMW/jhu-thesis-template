issn = {0001-8686},
journal = {Advances in colloid and interface science},
keywords = {Adsorption,Chemistry,Dietary Fats,Excipients,Excipients: chemistry,Food Additives,Food Additives: chemistry,Food Technology,Mechanical,Molecular Weight,Physical,Physicochemical Phenomena,Protein Conformation,Proteins,Proteins: chemistry,Rheology,Solubility,Stress,Surface Properties,Surface Tension,Surface-Active Agents,Surface-Active Agents: chemistry,Unsaturated,Viscosity,Water,Water: chemistry},
month = jul,
number = {3},
pages = {437--71},
pmid = {11511044},
title = {{Interfacial rheological properties of adsorbed protein layers and surfactants: a review.}},
